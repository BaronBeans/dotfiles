alias src='cd ~/code/'
alias code='code-insiders .'
alias reboot="shutdown.exe -r -t 0"
alias zombies="ps axo stat,ppid,pid,comm | grep -w defunct"
alias gk='gitkraken'
alias v='nvim'
alias vim='nvim'
alias ex='explorer.exe .'
alias update='sudo apt update && sudo apt -y upgrade'
alias t='tmux new -s $(basename "$PWD")'
alias ns='sudo vim /etc/resolv.conf'
alias notes='hx ~/code/personal/notes.md'
alias goog='ping google.com'
alias use='nvm use'
